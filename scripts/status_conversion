db.emaildatas.find().snapshot().forEach(
    function (doc) {
        let state="unused";
        if(doc.is_trash){
            state="trash"
        }else if(doc.is_moved){
            state="move"
        }else if(doc.is_keeped){
            state="keep"
        }else if(doc.is_delete){
            state="delete"
        }

        db.emaildatas.update(
                    {
                        _id: doc._id
                    },
                    {
                        $set: {
                            status:state,
                            status_date:new Date()
                        }
                    }
                );
    }
);